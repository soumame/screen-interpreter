# Google Gemini API Key
# Get your key from https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_api_key_here

# Obsidian Vault Name
# This is used to send the screen analysis to Obsidian
OBSIDIAN_VAULT_NAME=your_vault_name_here

# AFK Timeout in Minutes
# The application will skip screen interpretation if the user has been idle for this many minutes
AFK_TIMEOUT_MINUTES=5
